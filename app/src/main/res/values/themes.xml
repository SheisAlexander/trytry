<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.English" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/kdg</item>
        <item name="colorPrimaryVariant">@color/klg</item>
        <item name="colorOnPrimary">@color/km</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/kdg</item>
        <item name="colorSecondaryVariant">@color/km</item>
        <item name="colorOnSecondary">@color/km</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="AppBaseTheme" parent="android:Theme.Light"></style>

    <style name="AppTheme" parent="AppBaseTheme"></style>


    <style name="Theme.English.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.English.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.English.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="TransparentDialog" parent="Theme.AppCompat.Dialog">
        <item name="android:windowBackground">@color/transparent</item>
    </style>

</resources>